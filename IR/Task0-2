  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['https://api.github.com/notifications', 'https://api.github.com/notifications', 'https://api.github.com/notifications']\n"
     ]
    }
   ],
   "source": [
    "def res(url):\n",
    "    if not url:\n",
    "        return 'N/A'\n",
    "    r = requests.get(url)\n",
    "    jsobj = r.json()\n",
    "\n",
    "    if 'notifications_url' not in jsobj:\n",
    "        return 'N/A'\n",
    "    return jsobj['notifications_url']\n",
    "\n",
    "def main():\n",
    "    with concurrent.futures.ProcessPoolExecutor(max_workers=3) as executor:\n",
    "        futures = [executor.submit(res, url) for url in urls]\n",
    "        rtn = []\n",
    "        for future in concurrent.futures.as_completed(futures):\n",
    "            rtn.append(future.result())\n",
    "    \n",
    "    print(rtn)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    main()"
   ]
  },
